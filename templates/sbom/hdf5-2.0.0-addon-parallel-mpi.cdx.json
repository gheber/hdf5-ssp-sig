            "purl": "pkg:generic/${MPI_IMPLEMENTATION_PURL_NAME}@${MPI_VERSION}",
            "scope": "optional",
            "properties": [
                {
                    "name": "hdf5:feature",
                    "value": "parallel-hdf5"
                },
                {
                    "name": "hdf5:enabledWhen",
                    "value": "HDF5_ENABLE_PARALLEL=ON"
                }
            ],
            "hashes": [
                {
                    "alg": "SHA-256",
                    "content": "${MPI_SHA256}"
                }
            ]
        }
    ],
    "dependencies": [
        {
            "ref": "hdf5:addon:parallel-mpi@2.0.0",
            "dependsOn": [
                "hdf5:core@2.0.0",
                "pkg:generic/${MPI_IMPLEMENTATION_PURL_NAME}@${MPI_VERSION}"
